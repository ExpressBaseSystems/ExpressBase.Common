<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp2.0</TargetFramework>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="FOSS.ServiceStack.Core" Version="1.0.43" />
    <PackageReference Include="FOSS.ServiceStack.Redis.Core" Version="1.0.43" />
    <PackageReference Include="MongoDB.Driver" Version="2.4.4" />
    <PackageReference Include="MongoDB.Driver.GridFS" Version="2.4.4" />
    <PackageReference Include="Newtonsoft.Json" Version="10.0.3" />
    <PackageReference Include="Npgsql" Version="3.2.5" />
    <PackageReference Include="protobuf-net" Version="2.3.2" />
    <PackageReference Include="Twilio" Version="5.9.1" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="ServiceStack\" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Properties\Resources.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Resources.resx</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Properties\Resources.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>Resources.Designer.cs</LastGenOutput>
    </EmbeddedResource>
    <EmbeddedResource Include="SqlScripts/PostGreSql/DataDb/Create_Eb_User.sql" />
    <EmbeddedResource Include="SqlScripts/PostGreSql/DataDb/Alter_Eb_User_Sequences.sql" />
    <EmbeddedResource Include="SqlScripts/PostGreSql/DataDb/Create_Eb_User_Indexes.sql" />
    <EmbeddedResource Include="SqlScripts/PostGreSql/DataDb/Create_Eb_User_Functions.sql" />

    <EmbeddedResource Include="SqlScripts/PostGreSql/ObjectsDb/Create_Eb_Objects.sql" />
    <EmbeddedResource Include="SqlScripts/PostGreSql/ObjectsDb/Alter_Eb_Object_Sequences.sql" />
    <EmbeddedResource Include="SqlScripts/PostGreSql/ObjectsDb/Create_Eb_Object_Indexes.sql" />
    <EmbeddedResource Include="SqlScripts/PostGreSql/ObjectsDb/Create_Eb_Object_Functions.sql" />
  </ItemGroup>

  <ItemGroup>
    <!--<ProjectReference Include="..\ServiceStack.Core\ServiceStack.Redis\ServiceStack.Redis.csproj" />
    <ProjectReference Include="..\ServiceStack.Core\ServiceStack\ServiceStack.csproj" />-->
  </ItemGroup>

</Project>
