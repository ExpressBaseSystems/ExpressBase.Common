<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp2.0</TargetFramework>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="SqlScripts\ORACLE\InfraDb\**" />
    <Compile Remove="SqlScripts\ORACLE\ObjectsDb\Tables\**" />
